<nav class="navbar" role="navigation">

  <div class="grid-container">

    <div class="grid-30">

      <h1 class="brand">

        <a class="brand__link" href="{{site.html_url}}" alt="{{site.title}}">

          <span class="brand__name">{{site.name}}</span>

        </a>

      </h1>

    </div>

    <div class="grid-70">

      <div class="navbar-links">

        {{#each site.nav.links }}
        <a class="navbar-link" href="{{url}}" title="{{alt}}" rel="prefetch">{{text}}</a>
        {{/each}}

      </div>

    </div>

  </div>

</nav>
